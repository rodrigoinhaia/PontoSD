NODE_ENV=test

# Database
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_DATABASE=pontosdigital_test
DB_HOST=localhost
DB_PORT=5432

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# JWT
JWT_SECRET=test_jwt_secret_key_must_be_at_least_32_characters
JWT_EXPIRES_IN=1h
JWT_REFRESH_EXPIRES_IN=7d
REFRESH_TOKEN_SECRET=test_refresh_token_secret_key_must_be_at_least_32_characters
REFRESH_TOKEN_EXPIRES_IN=7d

# Email
SMTP_HOST=smtp.ethereal.email
SMTP_PORT=587
SMTP_USER=test@ethereal.email
SMTP_PASS=test_password
EMAIL_FROM=test@pontosd.com.br

# Geolocation
GOOGLE_MAPS_API_KEY=test_google_maps_api_key
MAX_DISTANCE_METERS=100

# Upload
UPLOAD_DIR=./uploads/test
MAX_FILE_SIZE=5242880

# Time
TIMEZONE=America/Sao_Paulo
WORKING_HOURS_START=08:00
WORKING_HOURS_END=18:00
LUNCH_BREAK_START=12:00
LUNCH_BREAK_END=13:00

# Log
LOG_LEVEL=debug
LOG_FILE=./logs/test.log

# Rate Limit
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# URLs
API_URL=http://localhost:3001
FRONTEND_URL=http://localhost:3000
CORS_ORIGIN=http://localhost:3000

# App
PORT=3001 